<Window x:Class="MyAppLauncher.DeleteSettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyAppLauncher"
        mc:Ignorable="d"
        Title="削除設定" Height="300" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="番号:"/>
            <TextBox x:Name="NumberBox" HorizontalAlignment="Left" Margin="0,5" Width="100"/>
            <Button Content="削除" Click="Delete_Setting" Margin="0,10"/>
        </StackPanel>
    </Grid>
</Window>
